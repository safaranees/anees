import java.io.*;
import java.util.*;
class Positive1
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int a=s.nextInt();
if(a>0)
{
System.out.println("the number is positive");
}
else if(a<0)
{
System.out.println("the number is negative");
}
else
{
System.out.println("the number is zero");
}
}
}
